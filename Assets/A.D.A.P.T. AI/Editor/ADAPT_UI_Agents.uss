#MainContainer 
{
	background-color: rgba(242, 246, 250,1);
	display: flex;
	flex-direction: column;
	width: auto;
	margin: 2%;
	text-color: rgb(51, 51, 51);
}
#SubContainer
{
    height: 100%;
    width: 100%;
    position: absolute;
    right: 0;
    top: 0;
	padding: 1%;
	background-color: rgba(194, 194, 194,1);
	border-width: 1px;
	border-color: rgba(90, 90, 90,1);
}

#SubContainer_Agents
{
    height: 50%;
    width: 100%;
    position: relative;
    right: 0;
    top: 0;
	padding: 1%;
	background-color: rgba(194, 194, 194,1);
	border-width: 1px;
	border-color: rgba(90, 90, 90,1);
}

#SubContainer_Actions
{
    height: 50%;
    width: 100%;
    position: relative;
    right: 0;
    top: 0;
	padding: 1%;
	background-color: rgba(194, 194, 194,1);
	border-width: 1px;
	border-color: rgba(90, 90, 90,1);
}

#ImageHolder 
{
	height: 40%;
	width: auto;
	display: flex;
	flex-direction: row;
}
#DescriptionPanel 
{
	border-width: 1px;
	border-color: rgba(0, 115, 135,1);
	-unity-text-align: middle-center;
	flex: 2;
	display: flex;
	justify-content: center;
	align-items: center;
}
#TitlePanel 
{
	border-width: 1px;
	border-color: rgba(0, 115, 135,1);
	-unity-text-align: middle-center;
	display: flex;
	flex: 1;
	justify-content: center;
	align-items: center;
}
#AgentsLabel 
{
	font-size: 25px;
	font-style: bold;
	color: rgba(6, 128, 162,1);
}
#DescriptionLabel 
{
	font-size: 15px;
}
#AgentSelectionLabel
{
	font-size: 20px;
	padding: 2%;
}
#SelectedObjectHolder
{
	font-size: 19px;
}
#ButtonPanel 
{
	border-width: 1px;
	border-color: rgba(0, 115, 135,1);
	-unity-text-align: middle-center;
	display: flex;
	flex: 1;
	justify-content: center;
	align-items: center;
}
#AgentButton 
{
	background-color: rgba(128, 252, 252,1);
	width:90%;
	font-size: 15px;
}
#AgentExistsLabel
{
	padding-top: 2%;
	text-color: red;
}

#ActionsLabel 
{
	font-size: 25px;
	font-style: bold;
	color: rgba(6, 128, 162,1);
}
#DescriptionActionLabel 
{
	font-size: 15px;
}
#ActionButton 
{
	background-color: rgba(128, 252, 252,1);
	width:90%;
	font-size: 15px;
}
#ActionName
{
	min-width: 100;
}
#ActionNameLabel
{
	font-size: 15px;
	font-style: bold;
	padding: 2%;
}
#ActionExistsLabel
{
	padding-top: 2%;
	text-color: red;
}
#ActionFileExistsLabel
{
	font-size: 11px;
	padding-top: 2%;
	text-color: red;
}
